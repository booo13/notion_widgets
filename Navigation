<body>

	<nav class="navMenu  border background">
		<a id="home" onclick="navigate(this.id)" href="#"> <i class="fa-duotone fa-house"></i></i> Home</a>
		<a id="properties" onclick="navigate(this.id)" href="#"> <i class="fa-duotone fa-house-user"></i> Properties</a>
		<a id="clients" onclick="navigate(this.id)" href="#"> <i class="fa-duotone fa-user"></i> Clients</a>
		<a id="sales" onclick="navigate(this.id)" href="#"> <i class="fa-duotone fa-wand-magic-sparkles"></i> Sales</a>
		<a id="analytics" onclick="navigate(this.id)" href="#"> <i class="fa-solid fa-filter"></i> Analytics</a>
		<a id="marketing" onclick="navigate(this.id)" href="#"> <i class="fa-solid fa-bolt"></i> Marketing</a>
		<a id="operations" onclick="navigate(this.id)" href="#"> <i class="fa-duotone fa-pen-to-square"></i> Operations</a>
		<a id="knowledge-hub" onclick="navigate(this.id)" href="#"> <i class="fa-duotone fa-bookmark"></i> Knowledge Hub</a>


	</nav>


	<script>

		let direction = "vertical";
		if (direction === "vertical") {
			document.head.insertAdjacentHTML("beforeend", `<style>.navMenu a {display: block; text-align:left; margin:7px} i{width:15px; margin-right:5px} .navMenu{margin:3px}</style>`)
		}


		function navigate(id) {

			if (id == 'home')
				window.open("https://www.notion.so/symbolrealty/Symbol-Realty-Dashboard-6bc96a24b85f4298be8327d2ec4b9292?pvs=4", '_blank').focus();
			else if (id == "properties")
				window.open("https://www.notion.so/symbolrealty/Properties-59a16c4fcfa441579fa4d76e372c35b4?pvs=4", '_blank').focus();
			else if (id == "clients")
				window.open("https://www.notion.so/symbolrealty/Clients-57b5535027584da688b298a1684e125d?pvs=4", '_blank').focus();
			else if (id == "sales")
				window.open("https://www.notion.so/symbolrealty/Sales-5cc538206f384339a2d3049396bb2526?pvs=4", '_blank').focus();
			else if (id == "analytics")
				window.open("https://www.notion.so/symbolrealty/Analytics-5ea9b2edde4f4a4fb753a2d2add26e07?pvs=4", '_blank').focus();
			else if (id == "marketing")
				window.open("https://www.notion.so/symbolrealty/Marketing-c1bc112a8a46481ebe1a5e2c61d5358d?pvs=4", '_blank').focus();
			else if (id == "operations")
				window.open("https://www.notion.so/symbolrealty/Operations-9e87a19b24a7409dabc4de07b33b7dbb?pvs=4", '_blank').focus();
			else if (id == "knowledge-hub")
				window.open("https://www.notion.so/symbolrealty/Knowledge-Hub-baac8d9a7e56477facb20f135f4d5f78?pvs=4", '_blank').focus();
		

		}
	</script>


